<!-- nofilter(TidyAll::Plugin::OTRS::TT::ScriptUsage) -->
<div class="MainBox ARIARoleMain LayoutFixedSidebar SidebarFirst Inventory">
    <h1>[% Translate("Manage Inventory") | html %]</h1>
#
# SidebarColumnLeft
#
    <div class="SidebarColumn">
#
# Filter
#
[% RenderBlockStart("FilterObject") %]
        <div class="WidgetSimple">
            <div class="Header">
                <h2><label for="FilterObject">[% Translate("Filter for Object") | html %]</label></h2>
            </div>
            <div class="Content">
                <input type="text" id="FilterObject" class="W95pc" name="FilterObject" value="" title="[% Translate(" Filter for Object") | html %]" />
            </div>
        </div>
[% RenderBlockEnd("FilterObject") %]
#
# Filter
#
#
# Search
#
[% RenderBlockStart("Search") %]
        <form action="[% Env(" CGIHandle") %]" method="post" name="search">
            <div class="WidgetSimple">
                <input type="hidden" name="Action" value="[% Env(" Action") %]" />
                <input type="hidden" name="Subaction" value="Search" />
                <div class="Header">
                    <h2><label for="SearchObject">[% Translate("Search for Object") | html %]</label></h2>
                </div>
                <div class="Content">
                    <input type="text" id="ID" class="W95pc" name="ID" value="[% Translate(Data.ID) | html %]" title="[% Translate(" UB-ID") | html %]" onfocus="if (this.value=='[% Translate(" UB-ID") | html %]') this.value='' ;" onblur="if (this.value=='') this.value='[% Translate(" UB-ID") | html %]';" />
                </div>
                <div class="Content">
                    <input type="text" id="Type" class="W95pc" name="Type" value="[% Translate(Data.Type) | html %]" title="[% Translate(" Type") | html %]" onfocus="if (this.value=='[% Translate(" Type") | html %]') this.value='' ;" onblur="if (this.value=='') this.value='[% Translate(" Type") | html %]';" />
                </div>
                <div class="Content">
                    <input type="text" id="Model" class="W95pc" name="Model" value="[% Translate(Data.Model) | html %]" title="[% Translate(" Model") | html %]" onfocus="if (this.value=='[% Translate(" Model") | html %]') this.value='' ;" onblur="if (this.value=='') this.value='[% Translate(" Model") | html %]';" />
                </div>
                <div class="Content">
                    <input type="text" id="Manufacturer" class="W95pc" name="Manufacturer" value="[% Translate(Data.Manufacturer) | html %]" title="[% Translate(" Manufacturer") | html %]" onfocus="if (this.value=='[% Translate(" Manufacturer") | html %]') this.value='' ;" onblur="if (this.value=='') this.value='[% Translate(" Manufacturer") | html %]';" />
                </div>
                <div class="Content">
                    <input type="text" id="Serialnumber" class="W95pc" name="Serialnumber" value="[% Translate(Data.Serialnumber) | html %]" title="[% Translate(" Serialnumber") | html %]" onfocus="if (this.value=='[% Translate(" Serialnumber") | html %]') this.value='' ;" onblur="if (this.value=='') this.value='[% Translate(" Serialnumber") | html %]';" />
                </div>
                <div class="Content">
                    <input type="text" id="SAP" class="W95pc" name="SAP" value="[% Translate(Data.SAP) | html %]" title="[% Translate(" SAP") | html %]" onfocus="if (this.value=='[% Translate(" SAP") | html %]') this.value='' ;" onblur="if (this.value=='') this.value='[% Translate(" SAP") | html %]';" />
                </div>
                <div class="Content">
                    <input type="text" id="Room" class="W95pc" name="Room" value="[% Translate(Data.Room) | html %]" title="[% Translate(" Room") | html %]" onfocus="if (this.value=='[% Translate(" Room") | html %]') this.value='' ;" onblur="if (this.value=='') this.value='[% Translate(" Room") | html %]';" />
                </div>
                <div class="Content">
                    <ul class="ActionList">
                        <li>
                            <button type="submit" class="CallForAction  Fullsize  Center" onclick="if (getElementById('ID').value=='[% Translate(" UB-ID") | html %]') getElementById('ID').value='' ; if (getElementById('Type').value=='[% Translate("Type") | html %]' ) getElementById('Type').value='' ; if (getElementById('Model').value=='[% Translate("Model") | html %]' ) getElementById('Model').value='' ; if (getElementById('Manufacturer').value=='[% Translate("Manufacturer") | html %]' ) getElementById('Manufacturer').value='' ; if (getElementById('Serialnumber').value=='[% Translate("Serialnumber") | html %]' ) getElementById('Serialnumber').value='' ; if (getElementById('SAP').value=='[% Translate("SAP") | html %]' ) getElementById('SAP').value='' ; if (getElementById('Room').value=='[% Translate("Room") | html %]' ) getElementById('Room').value='' ;">
                                <span>[% Translate("Search") | html %]</span>
                            </button>
                        </li>
                        <li>
                            <button type="submit" value="[% Translate(" reset") | html %]" class="CallForAction  Fullsize  Center" onclick="getElementById('ID').value='UB-ID'; getElementById('Type').value='Type'; getElementById('Model').value='Model'; getElementById('Manufacturer').value='Manufacturer'; getElementById('Serialnumber').value='Serialnumber'; getElementById('SAP').value='SAP'; getElementById('Room').value='Room'; "><span>[% Translate("Reset") | html %]</span>
                            </button>
                        </li>
                    </ul>
                </div>
            </div>
        </form>
[% RenderBlockEnd("Search") %]
#
# Search
#
#
# ActionList
#
[% RenderBlockStart("ActionList") %]
        <div class="WidgetSimple">
            <div class="Header">
                <h2>[% Translate("Actions") | html %]</h2>
            </div>
            <div class="Content">
                <ul class="ActionList">
                    <li>
                        <a href="[% Env(" Baselink") %]Action=[% Env("Action") %];Subaction=Overview;Limit=30;" class="CallForAction Fullsize Center"><span>[% Translate("Overview") | html %]</span></a>
                    </li>
                    <li>
                        <a href="[% Env(" Baselink") %]Action=[% Env("Action") %];Subaction=Overview;Limit=9999;" class="CallForAction Fullsize Center"><span>[% Translate("All") | html %]</span></a>
                    </li>
                    <li>
                        <a class="CallForAction Fullsize Center Plus" href="[% Env(" Baselink") %]Action=[% Env("Action") %];Subaction=Add">
                            <span>[% Translate("Add Object") | html %]</span>
                        </a>
                    </li>
                    <li>
                        <a href="[% Env(" Baselink") %][% Env("LastStatsView") %]" class="CallForAction Fullsize Center"><span>[% Translate("Go to overview") | html %]</span></a>
                    </li>
                </ul>
            </div>
        </div>
[% RenderBlockEnd("ActionList") %]
#
# ActionList
#
#
# SelectInfo
#
[% RenderBlockStart("SelectInfo") %]
        <div class="WidgetSimple">
            <div class="Header">
                <h2>[% Translate("Select Information") | html %]</h2>
            </div>
            <div class="Content">
                <fieldset class="TableLike FixedLabelSmall Narrow">
                    <label>[% Translate("selected key") | html %]:</label>
                    <p class="Value" title="[% Translate(Data.Key) | html %]">[% Translate(Data.Key) | html %]</p>
                    <div class="Clear"></div>
                    <label>[% Translate("selected value") | html %]:</label>
                    <p class="Value" title="[% Translate(Data.Value) | html %]">[% Translate(Data.Value) | html %]</p>
                    <div class="Clear"></div>
                    <label>[% Translate("count") | html %]:</label>
                    <p class="Value" title="[% Translate(Data.ObjectCount) | html %]">[% Translate(Data.ObjectCount) | html %]</p>
                    <div class="Clear"></div>
                </fieldset>
            </div>
        </div>
[% RenderBlockEnd("SelectInfo") %]
#
# SelectInfo
#
#
# Hints
#
[% RenderBlockStart("Hint") %]
        <div class="WidgetSimple">
            <div class="Header">
                <h2>[% Translate("Hint") | html %]</h2>
            </div>
            <div class="Content ">
                <p class="FieldExplanation">
                    [% Translate("Inventory management.") | html %]
                </p>
            </div>
        </div>
[% RenderBlockEnd("Hint") %]
[% RenderBlockStart("FormHint") %]
        <div class="WidgetSimple">
            <div class="Header">
                <h2> [% Translate("Hint - Add / Edit") | html %]</h2>
            </div>
            <div class="Content">
                <dl class="FieldExplanation">
                    <dt><code>[% Translate("Type") | html %]</code></dt>
                    <dd>
                        [% Translate("Type of the object.") | html %] <br>
                        ([% Translate("e. g.") | html %]
                        <code>[% Translate("PC/Monitor/Printer") | html %]</code>).
                    </dd>
                    <dt><code>[% Translate("Model") | html %]</code></dt>
                    <dd>
                        [% Translate("Model is the Version of the Object.") | html %]
                        <br>
                        ([% Translate("e. g.") | html %]
                        <code>[% Translate("PC 2307/Printer 0815") | html %]</code>).
                    </dd>
                    <dt><code>[% Translate("Manufacturer") | html %]</code></dt>
                    <dd>
                        [% Translate("Company that manufactures a particular item.") | html %]<br>
                        ([% Translate("e. g.") | html %]
                        <code>[% Translate("Dell") | html %]</code>).
                    </dd>
                    <dt><code>[% Translate("Serialnumber") | html %]</code></dt>
                    <dd>
                        [% Translate("String that is a unique identifier of an object.") | html %]<br>
                        ([% Translate("e. g.") | html %]
                        <code>[% Translate("12345-45678-23070") | html %]</code>).
                    </dd>
                    <dt><code>[% Translate("PurchaseTime") | html %]</code></dt>
                    <dd>
                        [% Translate("Date of purchase") | html %]. <br>
                        ([% Translate("e. g.") | html %]
                        <code>[% Translate("23.07.1988") | html %]</code>).
                    </dd>
                    <dt><code>[% Translate("Comment") | html %]</code></dt>
                    <dd>
                        [% Translate("Use the comment field for some additional information.") | html %]<br>
                        ([% Translate("e. g.") | html %]
                        <code>[% Translate("This notification informs all agents about the coming update of OTRS to version 3.3.3.") | html %]</code>).
                    </dd>
                </dl>
            </div>
        </div>
[% RenderBlockEnd("FormHint") %]
#
# Hints
