[% RenderBlockStart("Overview") %]
        <div class="WidgetSimple">
            <div class="Header">
                <div class="ActionMenu">
                    <div class="WidgetAction Settings">
<a href="#" id="Overview-Toggle" title="[% Translate(" Settings") | html %]">
                            <i class="fa fa-gear"></i>
                        </a>
                    </div>
                    [% WRAPPER JSOnDocumentComplete %]
                    <script type="text/javascript">
                    //<![CDATA[
Core.UI.RegisterToggleTwoContainer($('#Overview-Toggle'), $('#Overview-Setting'), $('#Overview'));
                    //]]>

                    </script>
                    [% END %]
                    <div class="Clear"></div>
                </div>
                <h2>[% Translate("Inventory") | html %]</h2>
            </div>
            <div class="Content">
                <div id="Overview-Setting" class="Hidden">
                    <form action="[% Env(" CGIHandle") %]" method="post" enctype="multipart/form-data" name="Overview-Setting-Form" id="Inventory[% Data.NameForm | html %]_setting_form" class="WidgetSettingsForm">
