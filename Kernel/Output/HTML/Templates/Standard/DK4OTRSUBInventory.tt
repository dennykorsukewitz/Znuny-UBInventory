                        <input type="hidden" name="Subaction" value="UpdateSettings" />
                        <input type="hidden" name="Name" value="OverviewSettings" />
                        <fieldset class="TableLike FixedLabelSmall DontPrint">
# <label for="[% Data.NamePref | html %]">
# [% Translate("Desc") | html %]:
# </label>
# <div class="Field DontPrint">
# [% Data.Option | html %]
# </div>
                            <ul>
                                <label>[% Translate("Shown Columns") | html %]</label>
                                <li>
                                    <span title="">
                                        <input type="checkbox" name="Type" id="Type" value="Type" checked="checked" />
                                        <label for="Settings-Type">[% Translate("Type") | html %]</label>
                                    </span>
                                </li>
                                <li>
                                    <span title="">
                                        <input type="checkbox" name="Model" id="Model" value="Model" checked="checked" />
                                        <label for="Settings-Model"> [% Translate("Model") | html %]</label>
