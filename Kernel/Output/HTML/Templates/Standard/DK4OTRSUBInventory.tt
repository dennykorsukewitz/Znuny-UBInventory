#
Core.Agent.Dashboard.RegisterUpdatePreferences($('#Overview-Submit'), 'Overview', $('#Overview-Setting-Form'));
Core.UI.RegisterToggleTwoContainer($('#Overview-Cancel'), $('#Overview-Setting'), $('#Overview'));
