                                <!--    <td><a href="[% Env("Baselink") %]Action=[% Env("Action") %];Subaction=Select;Key=create_time;Value=[% Data.CreateTime | uri %]">[% Data.CreateTime | Localize("Date") %]</a></td>
                        <td><a href="[% Env("Baselink") %]Action=[% Env("Action") %];Subaction=Select;Key=create_by;Value=[% Data.CreatedByID | uri %]">[% Data.CreatedByID | html  %]</a></td>
                        <td><a href="[% Env("Baselink") %]Action=[% Env("Action") %];Subaction=Select;Key=change_time;Value=[% Data.ChangeTime | uri %]">[% Data.ChangeTime | Localize("Date") %]</a></td>
                        <td><a href="[% Env("Baselink") %]Action=[% Env("Action") %];Subaction=Select;Key=change_by;Value=[% Data.ChangeByID | html %]">[% Data.ChangeByID | html  %]</a></td>
                      -->
                                <td>
                                    <a class="CallForAction Fullsize Center Delete" href="[% Env(" Baselink") %]Action=[% Env("Action") %];Subaction=Delete;ID=[% Data.ID | html %]"><span>[% Translate("Delete") | html %]</span></a>
                                </td>
                            </tr>
[% RenderBlockEnd("ObjectList") %]
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
    [% WRAPPER JSOnDocumentComplete %]
